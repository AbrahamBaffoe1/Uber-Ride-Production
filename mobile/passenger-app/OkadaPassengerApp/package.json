    "safe-start": "EXPO_NO_SYMLINKS=1 expo start --clear",
    "deep-clean": "rm -rf node_modules/.cache && watchman watch-del-all && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/haste-map-*",
    "fix-cpp": "node fix-cpp-exception.js",
    "start-safe": "EXPO_NO_SYMLINKS=1 EXPO_USE_HERMES=1 expo start --ios --clear",
    "start-fixed": "EXPO_NO_SYMLINKS=1 EXPO_USE_HERMES=1 EXPO_SKIP_BUNDLER_VALIDATION=1 expo start --ios --clear --no-dev --minify",
    "run-fixed": "./run-fixed-app.sh",
    "ios-simulator": "./run-ios-simulator.sh",
    "cpp-exception-free": "./run-cpp-exception-free.sh"
